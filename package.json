{
  "name": "vue-documenter",
  "version": "0.0.2",
  "description": "An automatic Vue component documenter - yes - a component that documents components",
  "main": "dist/vue-documenter.common.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name vue-documenter ./src/index.js",
    "serve": "vue-cli-service serve src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theianjohnson/vue-documenter"
  },
  "keywords": [
    "vuejs",
    "vue",
    "documenation",
    "dynamic analysis"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "author": "Ian Johnson",
  "license": "ISC",
  "devDependencies": {
    "@vue/cli": "^3.8.2",
    "@vue/cli-service": "^3.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "bootstrap": "^4.3.1"
  }
}
